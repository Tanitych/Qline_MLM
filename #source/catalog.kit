    <script>
      /*должен быть в HTML файле*/
      $(function () {
        $("#slider-range").slider({
          range: true,
          min: 0,
          max: 100000,
          values: [0, 30000],
          slide: function (event, ui) {
            $("#amount").val("от" + " " + ui.values[0] + "₸");
            $("#amount2").val("до" + " " + ui.values[1] + "₸");
          },
        });
        $("#amount").val(
          "от" + " " + $("#slider-range").slider("values", 0) + "₸"
        );
        $("#amount2").val(
          "до" + " " + $("#slider-range").slider("values", 1) + "₸"
        );
      });
    </script>
